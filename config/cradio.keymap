        lt_h: layer_tap_hold_preferred {
            #binding-cells = <2>;
            label = "layer_tap_hold_preferred"; 
            flavor = "hold-preferred";
            compatible = "zmk,behavior-hold-tap";
            tapping-term-ms = <200>;
            bindings = <&mo>, <&kp>;
        };

        // ht_sl: layer_tap_reversed {
        //     #binding-cells = <2>;
        //     label = "layer_tap_reversed"; 
        //     flavor = "hold-preferred";
