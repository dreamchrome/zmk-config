        // Key on hold, single tap layer on tap,
        l_or_key: layer_or_key {
            #binding-cells = <2>;
            label = "layer_or_key";
            flavor = "hold-preferred";
            compatible = "zmk,behavior-hold-tap";
            tapping-term-ms = <200>;
