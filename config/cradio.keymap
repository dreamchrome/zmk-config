        // The exceptions for LGUI
        q_or_app_switch: q_or_app_switch {
            compatible = "zmk,behavior-mod-morph";
            label = "q_or_app_switch";
            #binding-cells = <0>;
            bindings = <&kp Q>, <&kp TAB>;
