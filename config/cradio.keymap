        p_or_tab: p_or_tab {
            compatible = "zmk,behavior-mod-morph";
            label = "p_or_tab";
            #binding-cells = <0>;
            bindings = <&kp P>, <&kp TAB>;
            mods = <MOD_RGUI>;
