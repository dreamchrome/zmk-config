        // Mod-tap with hold-preferred: immediately activates modifier when another key is pressed
        mt_h: mod_tap_hold_preferred {
            #binding-cells = <2>;
            label = "mod_tap_hold_preferred";
            flavor = "hold-preferred";
            compatible = "zmk,behavior-hold-tap";
            tapping-term-ms = <200>;
            bindings = <&kp>, <&kp>;
        };

        // Backspace, but delete if shift is held
        // bpsc_or_del: bpsc_or_del {
